<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox alignment="CENTER" spacing="5.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
        fx:controller="de.saxsys.jfx.chattorama.MessageView">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
<children><VBox prefHeight="200.0" prefWidth="100.0" spacing="10.0">
<children><Label fx:id="nameLabel" style="-fx-font-weight: bold;" text="Name" /><Label fx:id="dateLabel" text="Datum" />
</children></VBox><TextArea fx:id="messageBox" />
</children>
</HBox>
