<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="400.0" prefWidth="650.0"
	spacing="5.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="de.saxsys.jfx.chattorama.ChatView">
	<padding>
		<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
	</padding>
	<children>
		<ListView fx:id="liste" prefHeight="200.0" prefWidth="200.0"
			VBox.vgrow="ALWAYS" />
		<VBox prefHeight="200.0" prefWidth="100.0" spacing="10.0">
			<children>
				<HBox spacing="10.0">
					<children>
						<Label text="Name" HBox.hgrow="SOMETIMES">
							<HBox.margin>
								<Insets />
							</HBox.margin>
							<padding>
								<Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
							</padding>
						</Label>
						<TextField fx:id="nameField" HBox.hgrow="SOMETIMES" />
					</children>
				</HBox>
				<TextArea fx:id="messageBox" prefHeight="200.0" prefWidth="200.0" />
				<Button fx:id="newButton" mnemonicParsing="false" text="Absenden" />
			</children>
		</VBox>
	</children>
</VBox>
