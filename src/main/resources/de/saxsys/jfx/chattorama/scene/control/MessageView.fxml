<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<fx:root type="javafx.scene.layout.HBox" alignment="CENTER" prefWidth="550.0" spacing="5.0"
	style="-fx-border-color: #eeeee;" xmlns="http://javafx.com/javafx/8"
	xmlns:fx="http://javafx.com/fxml/1">

	<padding>
		<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
	</padding>
	<children>
		<VBox minWidth="150.0" prefHeight="42.0" prefWidth="150.0"
			spacing="10.0" HBox.hgrow="NEVER">
			<children>
				<Label fx:id="nameLabel" style="-fx-font-weight: bold;" text="Name" />
				<Label fx:id="dateLabel" text="Datum" />
			</children>
		</VBox>
		<TextFlow fx:id="messageBox"
			style="-fx-background-color: white; -fx-border-color: #eeeeee;"
			HBox.hgrow="ALWAYS">
			<children>
				<Text strokeType="OUTSIDE" strokeWidth="0.0" />
			</children>
			<padding>
				<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
			</padding>
		</TextFlow>
	</children>
</fx:root>
